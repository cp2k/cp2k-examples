@SET RESTART_1            EXTERNAL_RESTART_WFN_1
@SET WFN_FILE_1           EXTERNAL_WFNRESTART_1

@SET DFT_FILE             dft-common-params.inc

@SET PROJECT_NAME         EXTERNAL_PROJNAME
@SET XYZFILE              EXTERNAL_XYZNAME

@SET PW_CUTOFF            300
@SET PW_REL_CUTOFF        50

@SET BECKE_ACTIVE         EXTERNAL_BECKE_ACTIVE
@SET BECKE_STR_1          EXTERNAL_BECKE_STR1
@SET BECKE_TARGET_1       EXTERNAL_BECKE_TARGET1

&GLOBAL
  PROJECT ${PROJECT_NAME}
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL

&FORCE_EVAL
  @SET WFN_FILE     ${WFN_FILE_1}
  @SET RESTART      ${RESTART_1}
  @SET NAME         ${PROJECT_NAME}
  @SET BECKE_TARGET ${BECKE_TARGET_1}
  @SET BECKE_STR    ${BECKE_STR_1}
  METHOD QS
  @include ${DFT_FILE}
  @include subsys.inc
&END FORCE_EVAL
